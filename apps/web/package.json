  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --max-warnings 0 .",
    "lint:fix": "eslint --fix .",
    "lint:report": "eslint --output-file=lint-report.json --format json .",
    "test": "jest",
    "test:ci": "jest --ci",
    "prepare": "pnpm --filter @sokosumi/database build && pnpm run generate:api-docs",
    "prisma:generate": "pnpm --filter @sokosumi/database prisma:generate",
    "prisma:migrate:dev": "pnpm --filter @sokosumi/database prisma:migrate:dev",
    "prisma:migrate:deploy": "pnpm --filter @sokosumi/database prisma:migrate:deploy",
    "prisma:migrate:reset": "pnpm --filter @sokosumi/database prisma:migrate:reset",
    "prisma:studio": "pnpm --filter @sokosumi/database prisma:studio",
    "format": "prettier --log-level silent --write \"src/**/*.{ts,tsx}\"",
    "generate:payment": "openapi-ts -f openapi-ts.payment.config.ts",
    "generate:registry": "openapi-ts -f openapi-ts.registry.config.ts",
    "generate:api": "pnpm run generate:registry && pnpm run generate:payment",
    "generate:api-docs": "next-openapi-gen generate"
  },

{
  "name": "sokosumi",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.19.0",
  "workspaces": [
    "web-app"
  ],
  "scripts": {
    "prepare": "husky || true",
    "dev": "pnpm -r dev",
    "sokosumi-web:dev": "pnpm --filter sokosumi-web dev",
    "build": "pnpm -r build",
    "sokosumi-web:build": "pnpm --filter sokosumi-web build",
    "start": "pnpm -r start",
    "sokosumi-web:start": "pnpm --filter sokosumi-web start",
    "lint": "pnpm -r lint",
    "sokosumi-web:lint": "pnpm --filter sokosumi-web lint",
    "lint:report": "pnpm -r lint:report",
    "sokosumi-web:lint:report": "pnpm --filter sokosumi-web lint:report",
    "test": "pnpm -r test",
    "sokosumi-web:test": "pnpm --filter sokosumi-web test",
    "test:ci": "pnpm -r test:ci",
    "sokosumi-web:test:ci": "pnpm --filter sokosumi-web test:ci",
    "sokosumi-web:prisma:migrate:deploy": "pnpm --filter sokosumi-web prisma:migrate:deploy",
    "sokosumi-web:prisma:migrate:dev": "pnpm --filter sokosumi-web prisma:migrate:dev"
  },
  "devDependencies": {
    "husky": "9.1.7"
  }
}

{
  "name": "sokosumi",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "workspaces": [
    "web-app"
  ],
  "scripts": {
    "prepare": "husky",
    "dev": "pnpm -r dev",
    "sokosumi-web:dev": "pnpm --filter sokosumi-web dev",
    "build": "pnpm -r build",
    "sokosumi-web:build": "pnpm --filter sokosumi-web build",
    "start": "pnpm -r start",
    "sokosumi-web:start": "pnpm --filter sokosumi-web start",
    "lint": "pnpm -r lint",
    "sokosumi-web:lint": "pnpm --filter sokosumi-web lint",
    "lint:report": "pnpm -r lint:report",
    "sokosumi-web:lint:report": "pnpm --filter sokosumi-web lint:report",
    "test": "pnpm -r test",
    "sokosumi-web:test": "pnpm --filter sokosumi-web test",
    "test:ci": "pnpm -r test:ci",
    "sokosumi-web:test:ci": "pnpm --filter sokosumi-web test:ci",
    "sokosumi-web:prisma:migrate:deploy": "pnpm --filter sokosumi-web prisma:migrate:deploy",
    "sokosumi-web:prisma:migrate:dev": "pnpm --filter sokosumi-web prisma:migrate:dev"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@hey-api/openapi-ts": "0.80.9",
    "@noble/hashes": "1.8.0",
    "@swc/core": "1.13.3",
    "@swc/helpers": "0.5.17",
    "@tailwindcss/postcss": "4.1.11",
    "@tailwindcss/typography": "0.5.16",
    "@testing-library/jest-dom": "6.6.4",
    "@testing-library/react": "16.3.0",
    "@types/jest": "30.0.0",
    "eslint": "9.33.0",
    "eslint-config-next": "15.4.6",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-no-relative-import-paths": "1.6.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "husky": "9.1.7",
    "jest": "30.0.5",
    "jest-environment-jsdom": "30.0.5",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "tailwindcss": "4.1.11",
    "tsx": "4.20.4",
    "typescript": "5.9.2"
  }
}

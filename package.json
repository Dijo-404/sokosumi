{
  "name": "sokosumi",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.20.0",
  "scripts": {
    "prepare": "husky || true",
    "dev": "pnpm -r dev",
    "sokosumi-web:dev": "pnpm --filter web dev",
    "build": "pnpm -r build",
    "sokosumi-web:build": "pnpm --filter web build",
    "start": "pnpm -r start",
    "sokosumi-web:start": "pnpm --filter web start",
    "lint": "pnpm -r lint",
    "sokosumi-web:lint": "pnpm --filter web lint",
    "lint:report": "pnpm -r lint:report",
    "sokosumi-web:lint:report": "pnpm --filter web lint:report",
    "test": "pnpm -r test",
    "sokosumi-web:test": "pnpm --filter web test",
    "test:ci": "pnpm -r test:ci",
    "sokosumi-web:test:ci": "pnpm --filter web test:ci",
    "sokosumi-web:prisma:migrate:deploy": "pnpm --filter web prisma:migrate:deploy",
    "sokosumi-web:prisma:migrate:dev": "pnpm --filter web prisma:migrate:dev"
  },
  "devDependencies": {
    "husky": "9.1.7"
  }
}

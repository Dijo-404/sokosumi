{
  "name": "sokosumi",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.12.1",
  "workspaces": [
    "web-app",
    "sync-function"
  ],
  "scripts": {
    "prepare": "husky",
    "dev": "pnpm -r dev",
    "sokosumi-web:dev": "pnpm --filter sokosumi-web dev",
    "build": "pnpm -r build",
    "masumi-sync:build": "pnpm --filter masumi-sync build",
    "sokosumi-web:build": "pnpm --filter sokosumi-web build",
    "start": "pnpm -r start",
    "sokosumi-web:start": "pnpm --filter sokosumi-web start",
    "lint": "pnpm -r lint",
    "sokosumi-web:lint": "pnpm --filter sokosumi-web lint",
    "masumi-sync:lint": "pnpm --filter masumi-sync lint",
    "lint:report": "pnpm -r lint:report",
    "sokosumi-web:lint:report": "pnpm --filter sokosumi-web lint:report",
    "masumi-sync:lint:report": "pnpm --filter masumi-sync lint:report",
    "test": "pnpm -r test",
    "sokosumi-web:test": "pnpm --filter sokosumi-web test",
    "masumi-sync:test": "pnpm --filter masumi-sync test",
    "test:ci": "pnpm -r test:ci",
    "sokosumi-web:test:ci": "pnpm --filter sokosumi-web test:ci",
    "masumi-sync:test:ci": "pnpm --filter masumi-sync test:ci",
    "sokosumi-web:prisma:migrate:deploy": "pnpm --filter sokosumi-web prisma:migrate:deploy",
    "sokosumi-web:prisma:migrate:dev": "pnpm --filter sokosumi-web prisma:migrate:dev",
    "masumi-sync:doctl:install": "pnpm --filter masumi-sync doctl:install",
    "masumi-sync:doctl:connect": "pnpm --filter masumi-sync doctl:connect --",
    "masumi-sync:doctl:deploy": "pnpm --filter masumi-sync doctl:deploy"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@hey-api/openapi-ts": "0.73.0",
    "@noble/hashes": "1.8.0",
    "@swc/core": "1.12.1",
    "@swc/helpers": "0.5.17",
    "@tailwindcss/postcss": "4.1.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/jest": "29.5.14",
    "eslint": "9.28.0",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-no-relative-import-paths": "1.6.1",
    "eslint-plugin-prettier": "5.4.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "husky": "^9.1.7",
    "jest": "30.0.0",
    "jest-environment-jsdom": "30.0.0",
    "postcss": "8.5.5",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.12",
    "tailwindcss": "4.1.10",
    "tsx": "4.20.3",
    "typescript": "5.8.3"
  }
}

    "embla-carousel-react": "8.6.0",
    "gravatar-url": "4.0.1",
    "input-otp": "1.4.2",
    "lucide-react": "0.525.0",
    "markdown-to-jsx": "7.7.10",
    "nanoid": "5.1.5",
    "next": "15.3.4",
    "next-intl": "4.3.3",
    "next-plausible": "3.12.4",
    "next-themes": "0.4.6",

    "@icons-pack/react-simple-icons": "13.3.0",
    "@prisma/client": "6.11.0",
    "@react-email/components": "0.1.1",
    "@sentry/nextjs": "9.34.0",
    "@tanstack/react-table": "8.21.3",
    "@tufjs/canonical-json": "2.0.0",
    "@usersnap/browser": "1.0.2",

{
  "name": "masumi-sync",
  "version": "1.0.0",
  "description": "DigitalOcean Function to sync Masumi services",
  "main": "packages/masumi-sync/main/main.js",
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "start": "tsx src/main.ts",
    "build": "tsc",
    "lint": "eslint src",
    "lint:report": "eslint src -f json -o lint-report.json",
    "format": "prettier --write src",
    "doctl:install": "doctl serverless install",
    "doctl:connect": "doctl serverless connect $namespace",
    "doctl:deploy": "doctl serverless deploy ."
  },
  "devDependencies": {
    "@types/node": "18.19.118"
  }
}
